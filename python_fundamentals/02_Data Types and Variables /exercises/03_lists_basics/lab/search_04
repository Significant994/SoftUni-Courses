n = int(input())
given_word = input()

all_words = []
list_with_given_word = []

for _ in range(n):
    new_string = input()
    all_words.append(new_string)

for string in all_words:
    if given_word in string:
        list_with_given_word.append(string)

print(all_words)
print(list_with_given_word)
